<module rename-to="actelionStructureViewer">
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User'/>
	
	<!-- Dependencies -->
	<inherits name="com.actelion.research.gwt.jre.Jre" />
    <inherits name="com.actelion.research.gwt.chemlib.ChemLib" />
    <inherits name="com.actelion.research.gwt.StructureViewer" />

    <source path="gui/editor"/>
    <entry-point class='com.actelion.research.gwt.gui.editor.GWTStructureEditor'/>
    <set-property name="user.agent" value="safari" />
</module>