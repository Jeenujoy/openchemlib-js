{
  "name": "openchemlib",
  "version": "5.6.0",
  "description": "Manipulate molecules",
  "keywords": [
    "chemistry",
    "molecule",
    "molfile",
    "SMILES"
  ],
  "scripts": {
    "copy:openchemlib": "gulp copy:openchemlib",
    "build": "gulp build:min && npm run build-pretty",
    "build-core": "gulp build:min -m core",
    "build-minimal": "gulp build:min -m minimal",
    "build-pretty": "gulp build:pretty -m full --suffix pretty",
    "eslint": "eslint __tests__ gulpfile.js openchemlib/classes.js",
    "eslint-fix": "npm run eslint -- --fix",
    "test": "npm run build && npm run test-only && npm run eslint",
    "test-only": "jest"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "main": "dist/openchemlib-core.js",
  "files": [
    "dist",
    "minimal.js",
    "core.js",
    "full.js"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/cheminfo/openchemlib-js.git"
  },
  "license": "BSD-3-Clause",
  "author": "MichaÃ«l Zasso",
  "contributors": [
    "Christian Rufener"
  ],
  "bugs": {
    "url": "https://github.com/cheminfo/openchemlib-js/issues"
  },
  "homepage": "https://github.com/cheminfo/openchemlib-js",
  "devDependencies": {
    "benchmark": "^2.1.4",
    "eslint": "^5.8.0",
    "eslint-config-cheminfo": "^1.18.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.27.1",
    "fs-extra": "^7.0.0",
    "gulp": "^3.9.1",
    "gwt-api-exporter": "^1.2.1",
    "jest": "^23.6.0",
    "minimist": "^1.2.0",
    "rimraf": "^2.6.2"
  }
}
