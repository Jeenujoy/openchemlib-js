  "contributors": [
    "Christian Rufener"
  ],
  "bugs": {
    "url": "https://github.com/cheminfo/openchemlib-js/issues"
  },
  "homepage": "https://github.com/cheminfo/openchemlib-js",
  "devDependencies": {
    "benchmark": "^2.1.1",
    "fs-extra": "^1.0.0",
    "gulp": "^3.9.1",
    "gwt-api-exporter": "^1.2.0",
    "minimist": "^1.2.0",
    "mocha": "^3.0.2",
    "mocha-better-spec-reporter": "^3.0.2",
    "rimraf": "^2.5.4",
    "should": "^11.1.0"
  }
